<!--pages/editWorkingTime/editWorkingTime.wxml-->
<view class="weui-cells weui-cells_after-title">  
  <view class="weui-cell weui-cell_select">
      <view class="weui-cell__hd weui-cell__hd_in-select-after">
          <view class="weui-label">上班时间</view>
      </view>
      <view class="weui-cell__bd">
          <picker bindchange="" value="{{startTimeIndex}}" range="{{startTime}}">
              <view class="weui-select weui-select_in-select-after">{{startTime[startTimeIndex]}}</view>
          </picker>
      </view>
  </view>

  <view class="weui-cell weui-cell_select">
      <view class="weui-cell__hd weui-cell__hd_in-select-after">
          <view class="weui-label">下班时间</view>
      </view>
      <view class="weui-cell__bd">
          <picker bindchange="" value="{{endTimeIndex}}" range="{{endTime}}">
              <view class="weui-select weui-select_in-select-after">{{endTime[endTimeIndex]}}</view>
          </picker>
      </view>
  </view>
</view>

<button style='margin-top:112rpx;' class="button button_margin_16" type="primary" bindtap="onSave">保存</button>